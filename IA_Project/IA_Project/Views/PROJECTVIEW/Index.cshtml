@model IEnumerable<IA_Project.PROJECT>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NAME_PROJECT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESC_PROJECT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.P_STATUS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.START_TIME)
