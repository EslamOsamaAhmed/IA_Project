@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NAME_PROJECT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESC_PROJECT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.P_STATUS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.START_TIME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.END_TIME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRICE)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PROJECT_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PROJECT_ID }) |
